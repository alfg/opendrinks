{
    "name": "Watermelon Smoothie",
    "description": "A simple, light, sweet and refreshing drink. Perfect for summer!",
    "github": "deeptiagg16",
    "ingredients": [
        "5 cups fresh watermelon",
        "1 lime",
        "1 cup frozen strawberries",
        "2 tbsp hemp seeds",
        "1/2 cup almond milk",
        "Sweetener of your choice"
    ],
    "directions": [
        "Place all ingredients in a blender and blend until smooth.",
        "Serve immediately."
    ],
    "image": "watermelon-smoothie.jpg",
    "keywords": [
       "watermelon",
       "non-alcoholic"
   ]
   
}
